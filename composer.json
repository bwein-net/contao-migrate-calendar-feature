{
  "name": "bwein-net/contao-migrate-calendar-feature",
  "description": "Migration of calendar_feature for Contao Open Source CMS",
  "keywords": [
    "contao",
    "migrate",
    "migration",
    "erdmannfreunde",
    "calendar",
    "feature"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/bwein-net/contao-migrate-calendar-feature",
  "license": "MIT",
  "authors": [
    {
      "name": "bwein.net",
      "homepage": "https://bwein.net/",
      "role": "Developer"
    },
    {
      "name": "Ralf Baussenwein",
      "homepage": "https://github.com/rabauss",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/bwein-net/contao-migrate-calendar-feature/issues",
    "source": "https://github.com/bwein-net/contao-migrate-calendar-feature"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/rabauss"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "contao/calendar-bundle": "^4.10",
    "symfony/config": "^4.4 || ^5.2",
    "symfony/dependency-injection": "^4.4 || ^5.2",
    "symfony/http-kernel": "^4.4 || ^5.2"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Bwein\\MigrateCalendarFeature\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Bwein\\MigrateCalendarFeature\\ContaoManager\\Plugin"
  },
  "scripts": {
    "ecs": [
      "vendor/bin/ecs check src/ --fix --ansi"
    ]
  }
}
